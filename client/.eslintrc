{
  "extends": ["airbnb", "plugin:prettier/recommended", "plugin:react-hooks/recommended"],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "ignorePatterns": ["node_modules/"],
  "rules": {
    "react/jsx-filename-extension": 0,
    "prettier/prettier": [2, {
      "endOfLine": "auto"
    }],
    "import/prefer-default-export": 0,
    "react/prop-types": 0
  }
}